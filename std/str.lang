# This part of the standard library defines string manupilation functions such as strlen #


strlen :: (string -> ptr) -> int {
    let len -> int = 0;

    let loop -> int = 0;
    while loop == 0 {
        if int[ptr(int(string) + len)] == 0 {
            loop = 1; # Exit Loop #
        }
        len = len + 1;
    }
    return len;
}


